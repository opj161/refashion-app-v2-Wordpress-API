            animate={{ opacity: 1, scale: 1 }}
            exit={{ opacity: 0, scale: 0.97 }}
            transition={{ duration: 0.3, ease: 'easeInOut' }}
            className="aspect-[2/3] w-full bg-muted rounded-md overflow-hidden relative pointer-events-none"
